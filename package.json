{
  "name": "kipu-mcp",
  "version": "0.2.0",
  "description": "MCP Server generated from OpenAPI spec for kipu-mcp",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "kipu-mcp": "build/index.js"
  },
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "start": "node build/index.js",
    "dev": "tsx src/index.ts",
    "build": "tsc && chmod 755 build/index.js",
    "typecheck": "tsc --noEmit",
    "prestart": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.0",
    "axios": "^1.9.0",
    "dotenv": "^16.4.5",
    "zod": "^3.24.3",
    "json-schema-to-zod": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.2",
    "typescript": "^5.8.3",
    "tsx": "^4.0.0"
  }
}